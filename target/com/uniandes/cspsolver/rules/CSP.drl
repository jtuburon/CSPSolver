			        sw|= ($op01.equals("*") && $op02.equals("+") && v2.getValue()== v1.getValue() * c1.getValue() && v3.getValue()== v1.getValue() + c2.getValue());
			        sw|= ($op01.equals("*") && $op02.equals("-") && v2.getValue()== v1.getValue() * c1.getValue() && v3.getValue()== v1.getValue() - c2.getValue());
